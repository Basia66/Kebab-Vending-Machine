<Page x:Class="Kebab_Vending_Machine.Page5"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kebab_Vending_Machine"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page5">

    <Grid>
        <DockPanel x:Name="dockPanel5" Margin="22,22,18,18">
            <Image x:Name="paulina5" HorizontalAlignment="Left" Source="Graphics/paulina.png" />
            <Canvas>
                <Image x:Name="bubble5" Width="300" Height="250" VerticalAlignment="Top" HorizontalAlignment="Right" Source="Graphics/bubble.png" />
                <TextBox x:Name="text5" Width="225" Height="148" Canvas.Left="28" Canvas.Top="15" HorizontalAlignment="Center" VerticalAlignment="Top" Visibility="Visible" Background="{x:Null}" BorderBrush="{x:Null}" IsReadOnly="True" IsEnabled="False" TextWrapping="Wrap"/>
            </Canvas>
        </DockPanel>
    </Grid>
</Page>
